{
  "name": "llm-core",
  "private": true,
  "scripts": {
    "start": "node dist/index.js",
    "build": "swc src -d dist --strip-leading-paths 1",
    "dev": "nodemon --exec \"node -r @swc-node/register\" src/index.ts",
    "migrate": "prisma migrate dev",
    "migrate:conversation-id": "npx tsx src/scripts/migrate-conversation-id-typo.ts",
    "nuke:figo-test": "npx tsx src/scripts/nuke-conversation.ts FIGO-TEST-1",
    "nuke:conversation": "npx tsx src/scripts/nuke-conversation.ts",
    "check:figo-test": "npx tsx src/scripts/dry-run-nuke-conversation.ts FIGO-TEST-1",
    "check:conversation": "npx tsx src/scripts/dry-run-nuke-conversation.ts",
    "test": "vitest",
    "test:firebase-storage": "vitest run src/tests/firebase-storage.test.ts",
    "test:firebase-watch": "vitest watch src/tests/firebase-storage.test.ts",
    "test:chat-service": "vitest run src/tests/chat-service.test.ts",
    "test:api-integration": "vitest run src/tests/api-integration.test.ts",
    "test:api-watch": "vitest watch src/tests/api-integration.test.ts",
    "test:unit": "vitest run src/tests/firebase-storage.test.ts src/tests/chat-service.test.ts",
    "test:integration": "vitest run src/tests/api-integration.test.ts",
    "test:all": "vitest run",
    "test:manual": "node test-image-chat.mjs",
    "test:image": "node test-image-chat.mjs",
    "test:image-analysis": "npx tsx src/scripts/test-image-analysis.ts",
    "compare:prompts": "npx tsx src/scripts/compare-system-prompts.ts",
    "studio": "prisma studio"
  },
  "devDependencies": {
    "@swc/cli": "^0.7.7",
    "@swc/core": "^1.12.5",
    "@types/amqplib": "^0.10.7",
    "@types/node": "^22.15.3",
    "nodemon": "^3.1.7",
    "prisma": "^6.10.1",
    "tsx": "4",
    "typescript": "5",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@elysiajs/node": "1",
    "@langchain/community": "^0.3.47",
    "@langchain/core": "^0.3.61",
    "@langchain/google-genai": "^0.2.13",
    "@langchain/langgraph": "^0.3.4",
    "@langchain/openai": "^0.5.15",
    "@langchain/pinecone": "^0.2.0",
    "@langchain/textsplitters": "^0.1.0",
    "@pinecone-database/pinecone": "^6.1.1",
    "@prisma/client": "^6.10.1",
    "@swc-node/register": "^1.10.10",
    "amqplib": "^0.10.8",
    "dotenv": "^16.5.0",
    "elysia": "1",
    "firebase": "^11.10.0",
    "firebase-admin": "^13.4.0",
    "logform": "^2.7.0",
    "parse-multipart-data": "^1.5.0",
    "tsyringe": "^4.10.0",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "zod": "^3.25.67"
  }
}